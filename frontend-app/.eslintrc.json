{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-var": "warn",
        "no-multiple-empty-lines": ["warn", { "max": 2 }],
        "no-duplicate-imports": "warn",
        "no-empty-function": "error",
        "prefer-const": "warn",
        "eqeqeq": "warn"
    }
}
